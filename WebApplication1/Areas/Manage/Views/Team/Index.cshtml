@using WebApplication1.Models
@model List<Team>
@{
    int count = 0;
}
<div>
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">FulName</th>
                <th scope="col">Description</th>
                <th scope="col">ImgUrl</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <th scope="row">@item.</th>
                    <td>@item.FullName</td>
                    <td>@item.Description</td>
                    <td><img  src="~\Upload\@item.ImgUrl" /></td>
                </tr>

            }

        </tbody>
    </table>
</div>
